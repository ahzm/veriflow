        [
          {
            "node": "Submit to Regulator A",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Submit to Regulator B",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Submit to Regulator A": {
      "main": [
        [
          {
            "node": "Send Confirmation Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Submit to Regulator B": {
      "main": [
