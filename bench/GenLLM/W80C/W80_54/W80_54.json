      }
    },
    {
      "id": "3",
      "name": "Validate Data",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "4",
      "name": "Submit to Regulator A",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Submitting data to Regulator A"
      }
    },
    {
      "id": "5",
      "name": "Submit to Regulator B",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Submitting data to Regulator B"
      }
    },
    {
      "id": "6",
      "name": "Send Confirmation Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "compliance@financialfirm.com",
        "subject": "Regulatory Submission Confirmation",
        "text": "The regulatory submissions have been completed successfully."
      }
    }
  ],
  "connections": {
    "Schedule Data Aggregation": {
      "main": [
