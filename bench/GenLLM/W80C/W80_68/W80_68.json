        "cronExpression": "0 9 * * 1"
      }
    },
    {
      "id": "2",
      "name": "Match Mentors and Mentees",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "const mentors = [/* array of mentors */];\nconst mentees = [/* array of mentees */];\nconst matches = mentors.map(mentor => {\n    const mentee = mentees[Math.floor(Math.random() * mentees.length)];\n    return { mentor, mentee };\n});\nreturn matches;"
      }
    },
    {
      "id": "3",
      "name": "Schedule Sessions",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "const matches = items.map(item => item.json);\nconst sessions = matches.map(match => {\n    return { session: `Session between ${match.mentor} and ${match.mentee}` };\n});\nreturn sessions;"
      }
    },
    {
      "id": "4",
      "name": "Send Email to Mentors",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
