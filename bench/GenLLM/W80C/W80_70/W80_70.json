{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Check Regulators",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "Check Regulators",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.regulators.com/check"
      }
    },
    {
      "id": "3",
      "name": "Detect Relevant Recalls",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return items.filter(item => item.json.recall === true);"
      }
    },
    {
      "id": "4",
      "name": "Notify Teams via Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "team@example.com",
        "subject": "Product Recall Notification",
        "text": "A relevant product recall has been detected. Please check the details."
      }
    },
    {
      "id": "5",
      "name": "Notify Teams via Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Attention: A relevant product recall has been detected. Check your email for details."
      }
    }
  ],
  "connections": {
    "Schedule Check Regulators": {
      "main": [
        [
          {
            "node": "Check Regulators",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check Regulators": {
      "main": [
        [
          {
            "node": "Detect Relevant Recalls",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Detect Relevant Recalls": {
      "main": [
        [
          {
            "node": "Notify Teams via Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Notify Teams via Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}