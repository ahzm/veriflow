{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Collect Indicators",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.ngo.org/indicators"
      }
    },
    {
      "id": "3",
      "name": "Validate Indicators",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "4",
      "name": "Aggregate Data",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "5",
      "name": "Send Report",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "report@ngo.org",
        "subject": "Monthly Indicators Report",
        "text": "Please find attached the aggregated indicators report."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Collect Indicators",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Collect Indicators": {
      "main": [
        [
          {
            "node": "Validate Indicators",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Validate Indicators": {
      "main": [
        [
          {
            "node": "Aggregate Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Aggregate Data": {
      "main": [
        [
          {
            "node": "Send Report",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}