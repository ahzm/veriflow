{
  "nodes": [
    {
      "id": "1",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "parameters": {
        "httpMethod": "POST",
        "path": "accreditation-evidence"
      }
    },
    {
      "id": "2",
      "name": "Collect Documents",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/collect-documents"
      }
    },
    {
      "id": "3",
      "name": "Map to Standards",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return items.map(item => ({ json: { mapped: item.json.document + ' mapped to standards' } }));"
      }
    },
    {
      "id": "4",
      "name": "Generate Submission Package",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return items.map(item => ({ json: { package: item.json.mapped + ' packaged for submission' } }));"
      }
    },
    {
      "id": "5",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "recipient@example.com",
        "subject": "Accreditation Submission Package",
        "text": "Your accreditation submission package is ready."
      }
    },
    {
      "id": "6",
      "name": "Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Accreditation submission package has been generated."
      }
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Collect Documents",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Collect Documents": {
      "main": [
        [
          {
            "node": "Map to Standards",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Map to Standards": {
      "main": [
        [
          {
            "node": "Generate Submission Package",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Generate Submission Package": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          },
          {
            "node": "Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}