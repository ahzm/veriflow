{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Event",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Territories",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/territories"
      }
    },
    {
      "id": "3",
      "name": "Fetch Platforms",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/platforms"
      }
    },
    {
      "id": "4",
      "name": "Fetch Sublicenses",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/sublicenses"
      }
    },
    {
      "id": "5",
      "name": "Send Summary Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "admin@example.com",
        "subject": "Daily Cross-Media Rights Summary",
        "text": "Summary of territories, platforms, and sublicenses fetched."
      }
    },
    {
      "id": "6",
      "name": "Send Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Daily rights management update completed."
      }
    }
  ],
  "connections": {
    "Schedule Event": {
      "main": [
        [
          {
            "node": "Fetch Territories",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Territories": {
      "main": [
        [
          {
            "node": "Fetch Platforms",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Platforms": {
      "main": [
        [
          {
            "node": "Fetch Sublicenses",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Sublicenses": {
      "main": [
        [
          {
            "node": "Send Summary Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Send Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}