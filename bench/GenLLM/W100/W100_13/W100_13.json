{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule CO2 Check",
      "type": "n8n-nodes-base.cron",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "Get CO2 Data",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/co2"
      }
    },
    {
      "id": "3",
      "name": "Send CO2 Alert to Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Current CO2 level is above the threshold!"
      }
    }
  ],
  "connections": {
    "Schedule CO2 Check": {
      "main": [
        [
          {
            "node": "Get CO2 Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Get CO2 Data": {
      "main": [
        [
          {
            "node": "Send CO2 Alert to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}