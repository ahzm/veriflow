{
  "nodes": [
    {
      "id": "1",
      "name": "Shopify Trigger",
      "type": "n8n-nodes-base.shopifyTrigger",
      "parameters": {}
    },
    {
      "id": "2",
      "name": "HTTP Request",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/shipping",
        "method": "GET",
        "responseFormat": "json",
        "options": {}
      }
    },
    {
      "id": "3",
      "name": "Generate Invoice PDF",
      "type": "n8n-nodes-base.pdfGenerate",
      "parameters": {
        "template": "Invoice Template",
        "data": {}
      }
    },
    {
      "id": "4",
      "name": "Dropbox Upload",
      "type": "n8n-nodes-base.dropbox",
      "parameters": {
        "operation": "upload",
        "filePath": "/invoices/{{ $json[\"invoiceId\"] }}.pdf",
        "binaryData": true
      }
    },
    {
      "id": "5",
      "name": "Send Confirmation Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "customer@example.com",
        "subject": "Your Invoice is Ready",
        "text": "Your invoice is ready for download: {{ $json[\"downloadLink\"] }}"
      }
    }
  ],
  "connections": {
    "Shopify Trigger": {
      "main": [
        [
          {
            "node": "HTTP Request",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request": {
      "main": [
        [
          {
            "node": "Generate Invoice PDF",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Generate Invoice PDF": {
      "main": [
        [
          {
            "node": "Dropbox Upload",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dropbox Upload": {
      "main": [
        [
          {
            "node": "Send Confirmation Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}