{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Rate Change",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "HTTP Request for Rate Update",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.telecom.com/rate-update"
      }
    },
    {
      "id": "3",
      "name": "Send Rate Change Notification",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "admin@telecom.com",
        "subject": "Rate Change Notification",
        "text": "The telecom rate has been updated."
      }
    },
    {
      "id": "4",
      "name": "Slack Notification for Rate Change",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "A new rate change has been implemented."
      }
    },
    {
      "id": "5",
      "name": "Test Activation",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.telecom.com/test-activation"
      }
    },
    {
      "id": "6",
      "name": "Send Test Activation Result",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "admin@telecom.com",
        "subject": "Test Activation Result",
        "text": "The test activation has been completed."
      }
    }
  ],
  "connections": {
    "Schedule Rate Change": {
      "main": [
        [
          {
            "node": "HTTP Request for Rate Update",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request for Rate Update": {
      "main": [
        [
          {
            "node": "Send Rate Change Notification",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Slack Notification for Rate Change",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send Rate Change Notification": {
      "main": [
        [
          {
            "node": "Test Activation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Slack Notification for Rate Change": {
      "main": [
        [
          {
            "node": "Test Activation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Test Activation": {
      "main": [
        [
          {
            "node": "Send Test Activation Result",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}