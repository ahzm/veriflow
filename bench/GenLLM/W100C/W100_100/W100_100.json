{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "HTTP Request - Observation Data",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.observationnetwork.org/data"
      }
    },
    {
      "id": "3",
      "name": "Process Data",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return items.map(item => { return { json: { processedData: item.json.data } }; });"
      }
    },
    {
      "id": "4",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "biodiversity@example.com",
        "subject": "New Biodiversity Observation Data",
        "text": "New observation data has been processed and is ready for review."
      }
    },
    {
      "id": "5",
      "name": "Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "New biodiversity observation data has been processed."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "HTTP Request - Observation Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request - Observation Data": {
      "main": [
        [
          {
            "node": "Process Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Process Data": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}