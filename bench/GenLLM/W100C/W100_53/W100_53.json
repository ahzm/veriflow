{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Title Search",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Title Search HTTP Request",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.landregistry.gov/title-search"
      }
    },
    {
      "id": "3",
      "name": "Check Liens",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.landregistry.gov/check-liens"
      }
    },
    {
      "id": "4",
      "name": "Register Title",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.landregistry.gov/register-title"
      }
    },
    {
      "id": "5",
      "name": "Send Confirmation Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "user@example.com",
        "subject": "Land Registry Transaction Confirmation",
        "text": "Your land registry transaction has been successfully processed."
      }
    },
    {
      "id": "6",
      "name": "Notify via Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "A new land registry transaction has been completed."
      }
    }
  ],
  "connections": {
    "Schedule Title Search": {
      "main": [
        [
          {
            "node": "Title Search HTTP Request",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Title Search HTTP Request": {
      "main": [
        [
          {
            "node": "Check Liens",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check Liens": {
      "main": [
        [
          {
            "node": "Register Title",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Register Title": {
      "main": [
        [
          {
            "node": "Send Confirmation Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Notify via Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}