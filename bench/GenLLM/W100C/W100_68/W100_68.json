{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Eligibility Check",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Check Eligibility",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.utilitycompany.com/check-eligibility"
      }
    },
    {
      "id": "3",
      "name": "Send Enrollment Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "customer@example.com",
        "subject": "Enrollment Confirmation",
        "text": "You have been successfully enrolled in the utility assistance program."
      }
    },
    {
      "id": "4",
      "name": "Send Recertification Reminder",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Reminder: It's time to recertify your eligibility for the utility assistance program."
      }
    }
  ],
  "connections": {
    "Schedule Eligibility Check": {
      "main": [
        [
          {
            "node": "Check Eligibility",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check Eligibility": {
      "main": [
        [
          {
            "node": "Send Enrollment Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Send Recertification Reminder",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}