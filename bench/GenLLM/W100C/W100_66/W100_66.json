        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Partners",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/partners"
      }
    },
    {
      "id": "3",
      "name": "Fetch Earn/Burn Rules",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/rules"
      }
    },
    {
      "id": "4",
      "name": "Reconcile Data",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "5",
      "name": "Send Report",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "admin@example.com",
        "subject": "Daily Loyalty Program Report",
        "text": "The daily report for the loyalty coalition programs has been generated."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Partners",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Partners": {
      "main": [
        [
          {
            "node": "Fetch Earn/Burn Rules",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Earn/Burn Rules": {
      "main": [
        [
          {
            "node": "Reconcile Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Reconcile Data": {
      "main": [
        [
          {
            "node": "Send Report",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}