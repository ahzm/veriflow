{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Feed Collection",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "Collect Threat Feeds",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://example.com/threat-feeds"
      }
    },
    {
      "id": "3",
      "name": "Normalize Data",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "4",
      "name": "Deduplicate IOCs",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "5",
      "name": "Share IOCs via Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "security-team@example.com",
        "subject": "New IOCs from Threat Intelligence",
        "text": "Please find the attached IOCs from the latest threat feeds."
      }
    },
    {
      "id": "6",
      "name": "Share IOCs via Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "New IOCs have been ingested and are available for review."
      }
    }
  ],
  "connections": {
    "Schedule Feed Collection": {
      "main": [
        [
          {
            "node": "Collect Threat Feeds",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Collect Threat Feeds": {
      "main": [
        [
          {
            "node": "Normalize Data",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Data": {
      "main": [
        [
          {
            "node": "Deduplicate IOCs",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Deduplicate IOCs": {
      "main": [
        [
          {
            "node": "Share IOCs via Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Share IOCs via Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}