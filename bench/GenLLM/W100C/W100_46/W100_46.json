{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch LCs",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/lcs"
      }
    },
    {
      "id": "3",
      "name": "Fetch Invoices",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/invoices"
      }
    },
    {
      "id": "4",
      "name": "Fetch Transport Documents",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/transport-docs"
      }
    },
    {
      "id": "5",
      "name": "Send Email Notification",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "trade@example.com",
        "subject": "New Trade Documents Available",
        "text": "New LCs, invoices, and transport documents have been fetched."
      }
    },
    {
      "id": "6",
      "name": "Send Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "New trade finance documents have been processed."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch LCs",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch LCs": {
      "main": [
        [
          {
            "node": "Fetch Invoices",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Invoices": {
      "main": [
        [
          {
            "node": "Fetch Transport Documents",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Transport Documents": {
      "main": [
        [
          {
            "node": "Send Email Notification",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Send Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}