{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Request",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "HTTP Request for Requests",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.pensionrequests.com/getRequests"
      }
    },
    {
      "id": "3",
      "name": "Verify Requests",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return items.filter(item => item.json.status === 'pending');"
      }
    },
    {
      "id": "4",
      "name": "Transfer Pension",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.pensiontransfer.com/transfer"
      }
    },
    {
      "id": "5",
      "name": "Send Confirmation Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "user@example.com",
        "subject": "Pension Transfer Confirmation",
        "text": "Your pension transfer has been successfully processed."
      }
    },
    {
      "id": "6",
      "name": "Send Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "A new pension transfer has been initiated."
      }
    }
  ],
  "connections": {
    "Schedule Request": {
      "main": [
        [
          {
            "node": "HTTP Request for Requests",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request for Requests": {
      "main": [
        [
          {
            "node": "Verify Requests",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Verify Requests": {
      "main": [
        [
          {
            "node": "Transfer Pension",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Transfer Pension": {
      "main": [
        [
          {
            "node": "Send Confirmation Email",
            "type": "main",
            "index": 0
          },
          {
            "node": "Send Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}