{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 8 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Weather",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.weatherapi.com/v1/current.json?key=YOUR_API_KEY&q=YOUR_LOCATION"
      }
    },
    {
      "id": "3",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "your_email@example.com",
        "subject": "Today's Weather Summary",
        "text": "Temperature: {{$json[\"current\"][\"temp_c\"]}}Â°C\nRain Probability: {{$json[\"current\"][\"precip_mm\"]}} mm"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Weather",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Weather": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}