{
  "nodes": [
    {
      "id": "1",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "parameters": {}
    },
    {
      "id": "2",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "={{$json[\"email\"]}}",
        "subject": "Welcome!",
        "text": "Welcome to our service! We're glad to have you."
      }
    },
    {
      "id": "3",
      "name": "Log Payload",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://example.com/log",
        "method": "POST",
        "jsonParameters": true,
        "options": {},
        "body": {
          "payload": "={{$json}}"
        }
      }
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Log Payload",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}