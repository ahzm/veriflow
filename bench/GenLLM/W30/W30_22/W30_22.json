{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 8 * * *"
      }
    },
    {
      "id": "2",
      "name": "Check Pollen Level",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.pollenlevel.com/check"
      }
    },
    {
      "id": "3",
      "name": "Send Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Pollen levels are high today!"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Check Pollen Level",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Check Pollen Level": {
      "main": [
        [
          {
            "node": "Send Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}