{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Sunrise/Sunset",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.sunrise-sunset.org/json?lat=36.7201600&lng=-4.4203400"
      }
    },
    {
      "id": "3",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "example@example.com",
        "subject": "Sunrise/Sunset Summary",
        "text": "Here are the sunrise and sunset times."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Sunrise/Sunset",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Sunrise/Sunset": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}