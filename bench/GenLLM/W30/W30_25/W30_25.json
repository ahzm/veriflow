{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 21 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Crypto Market Summary",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.coingecko.com/api/v3/global"
      }
    },
    {
      "id": "3",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "your_email@example.com",
        "subject": "Daily Crypto Market Summary",
        "text": "Here is the daily crypto market summary."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Crypto Market Summary",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Crypto Market Summary": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}