{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Error Logs",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://example.com/api/error-logs"
      }
    },
    {
      "id": "3",
      "name": "Parse and Classify Errors",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "4",
      "name": "Cluster Similar Errors",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "5",
      "name": "Create Bug Tickets in Jira",
      "type": "n8n-nodes-base.jira",
      "parameters": {
        "projectKey": "BUG",
        "summary": "Aggregated Bug Ticket",
        "description": "Aggregated description of errors",
        "issueType": "Bug"
      }
    },
    {
      "id": "6",
      "name": "Post to Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Top recurring issues: [list of issues]"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Error Logs",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Error Logs": {
      "main": [
        [
          {
            "node": "Parse and Classify Errors",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Parse and Classify Errors": {
      "main": [
        [
          {
            "node": "Cluster Similar Errors",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Cluster Similar Errors": {
      "main": [
        [
          {
            "node": "Create Bug Tickets in Jira",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Create Bug Tickets in Jira": {
      "main": [
        [
          {
            "node": "Post to Slack",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}