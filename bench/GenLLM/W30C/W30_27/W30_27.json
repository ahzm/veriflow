      "parameters": {}
    },
    {
      "id": "7",
      "name": "Send Summary Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "team@example.com",
        "subject": "Weekly Performance Summary",
        "text": "Here is the summary of the week's performance across channels."
      }
    }
