      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://graph.facebook.com/v10.0/me/feed",
        "method": "POST",
        "body": {
          "message": "Your adapted content for Facebook"
        }
      }
    },
    {
      "id": "4",
      "name": "Post to Twitter",
