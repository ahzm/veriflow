{
  "nodes": [
    {
      "id": "1",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "parameters": {}
    },
    {
      "id": "2",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "customer@example.com",
        "subject": "Thank you for your message",
        "text": "We have received your message and will get back to you shortly."
      }
    },
    {
      "id": "3",
      "name": "Log to HTTP",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://example.com/log",
        "method": "POST",
        "jsonParameters": true,
        "options": {},
        "body": {
          "message": "Customer message received"
        }
      }
    }
  ],
  "connections": {
    "Webhook": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          },
          {
            "node": "Log to HTTP",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}