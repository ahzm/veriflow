{
  "nodes": [
    {
      "id": "1",
      "name": "Cron Trigger",
      "type": "n8n-nodes-base.cron",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "HTTP CO2 Check",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.co2check.com/current"
      }
    },
    {
      "id": "3",
      "name": "Slack Notification",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Alert: CO2 levels are above 800ppm!"
      }
    }
  ],
  "connections": {
    "Cron Trigger": {
      "main": [
        [
          {
            "node": "HTTP CO2 Check",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP CO2 Check": {
      "main": [
        [
          {
            "node": "Slack Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}