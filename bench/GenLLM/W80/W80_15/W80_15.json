{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Backup",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 * * *"
      }
    },
    {
      "id": "2",
      "name": "Send Backup Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "recipient@example.com",
        "subject": "Midnight Backup Completed",
        "text": "The backup has been successfully completed at midnight."
      }
    }
  ],
  "connections": {
    "Schedule Backup": {
      "main": [
        [
          {
            "node": "Send Backup Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}