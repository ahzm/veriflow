{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 */3 * * *"
      }
    },
    {
      "id": "2",
      "name": "Fetch Volcano Alerts",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://example.com/volcano-alerts"
      }
    },
    {
      "id": "3",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "alerts@example.com",
        "subject": "Volcano Alerts",
        "text": "New volcano alerts have been fetched."
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Fetch Volcano Alerts",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Fetch Volcano Alerts": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}