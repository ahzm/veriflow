{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 8 * * *"
      }
    },
    {
      "id": "2",
      "name": "HTTP Request",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://mockapi.com/projects/tasks?status=BLOCKED",
        "responseFormat": "json"
      }
    },
    {
      "id": "3",
      "name": "Slack",
      "type": "n8n-nodes-base.slack",
      "parameters": {
        "text": "Daily report of blocked tasks has been generated."
      }
    },
    {
      "id": "4",
      "name": "Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "{{$json[\"ownerEmail\"]}}",
        "subject": "Reminder: Your Task is Blocked",
        "text": "Please check the status of your task."
      }
    },
    {
      "id": "5",
      "name": "Database Log",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "console.log('Notification sent to: ' + $json[\"ownerEmail\"]);"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "HTTP Request",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HTTP Request": {
      "main": [
        [
          {
            "node": "Slack",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Email",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Database Log",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Slack": {
      "main": [
        [
          {
            "node": "Database Log",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Email": {
      "main": [
        [
          {
            "node": "Database Log",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}