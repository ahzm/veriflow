{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule Payroll Tax Calculation",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 0 1 * *"
      }
    },
    {
      "id": "2",
      "name": "Compute Tax Liabilities",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return [{ json: { liabilities: 1000 } }];"
      }
    },
    {
      "id": "3",
      "name": "Generate Tax Filings",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return [{ json: { filing: 'Filing Data' } }];"
      }
    },
    {
      "id": "4",
      "name": "Submit Tax Filing via API",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.taxfiling.com/submit",
        "method": "POST",
        "jsonParameters": true,
        "options": {},
        "body": {
          "filing": "={{$json[\"filing\"]}}"
        }
      }
    },
    {
      "id": "5",
      "name": "Store Receipt",
      "type": "n8n-nodes-base.function",
      "parameters": {
        "functionCode": "return [{ json: { receipt: 'Receipt Data' } }];"
      }
    },
    {
      "id": "6",
      "name": "Send Confirmation Email",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "finance@company.com",
        "subject": "Payroll Tax Filing Confirmation",
        "text": "Your payroll tax filing has been successfully submitted."
      }
    }
  ],
  "connections": {
    "Schedule Payroll Tax Calculation": {
      "main": [
        [
          {
            "node": "Compute Tax Liabilities",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Compute Tax Liabilities": {
      "main": [
        [
          {
            "node": "Generate Tax Filings",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Generate Tax Filings": {
      "main": [
        [
          {
            "node": "Submit Tax Filing via API",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Submit Tax Filing via API": {
      "main": [
        [
          {
            "node": "Store Receipt",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Store Receipt": {
      "main": [
        [
          {
            "node": "Send Confirmation Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}