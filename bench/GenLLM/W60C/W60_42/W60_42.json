{
  "nodes": [
    {
      "id": "1",
      "name": "Schedule",
      "type": "n8n-nodes-base.schedule",
      "parameters": {
        "cronExpression": "0 * * * *"
      }
    },
    {
      "id": "2",
      "name": "Detect Signals",
      "type": "n8n-nodes-base.function",
      "parameters": {}
    },
    {
      "id": "3",
      "name": "Send Targeted Offer",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "to": "customer@example.com",
        "subject": "Exclusive Offer Just for You!",
        "text": "We have a special offer that we think you'll love!"
      }
    },
    {
      "id": "4",
      "name": "Track Conversion",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.example.com/track-conversion"
      }
    }
  ],
  "connections": {
    "Schedule": {
      "main": [
        [
          {
            "node": "Detect Signals",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Detect Signals": {
      "main": [
        [
          {
            "node": "Send Targeted Offer",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send Targeted Offer": {
      "main": [
        [
          {
            "node": "Track Conversion",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}