{
    "nodes": [
        {
            "id": "1",
            "name": "Cron",
            "type": "n8n-nodes-base.cron",
            "parameters": {
                "cronExpression": "*/3 * * * *"
            }
        },
        {
            "id": "2",
            "name": "HTTP",
            "type": "n8n-nodes-base.httpRequest",
            "parameters": {
                "url": "https://example.com"
            }
        },
        {
            "id": "3",
            "name": "Email Dot Notation",
            "type": "n8n-nodes-base.emailSend",
            "parameters": {
                "to": "user@example.com",
                "subject": "Status {{$json.statusCode}}"
            }
        }
    ],
    "connections": {
        "Cron": {
            "main": [
                [
                    {
                        "node": "HTTP"
                    }
                ]
            ]
        },
        "HTTP": {
            "main": [
                [
                    {
                        "node": "Email Dot Notation"
                    }
                ]
            ]
        }
    }
}